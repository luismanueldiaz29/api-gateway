{
    "endpoints": [
      {
        "endpoint": "/api/login",
        "output_encoding": "no-op",
        "method": "POST",
        "backend": [
          {
            "host_env": [
              "AUTH_HOST"
            ],
            "url_pattern": "/api/login",
            "extra_config": {
              "qos/circuit-breaker": {
                "interval": 60,
                "timeout": 10,
                "max_errors": 1,
                "name": "CB-auth-login-endpoint",
                "log_status_change": true
              }
            }
          }
        ]
      }
    ]
  }
  